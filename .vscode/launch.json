{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "/home/fordji/go-networking",
            "env": {
                "SERVER_PORT": "8080",
                "SERVER_HOST": "localhost",
                "GIN_MODE": "debug",
                "TCP_SERVER_PORT": "8081",
                "TCP_SERVER_HOST": "localhost",
                "FILE_STORE_PATH": "/home/xiaosi/go-networking/upload/",
                "CREDENTIAL_FILE_PATH": ""
            },
            "args": [
                "--check-go-version=false"
            ]
        },
        {
            "name": "(gdb) Launch",
            "type": "cppdbg",
            "request": "launch",
            "program": "enter program name, for example ${workspaceFolder}/a.out",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "Set Disassembly Flavor to Intel",
                    "text": "-gdb-set disassembly-flavor intel",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}